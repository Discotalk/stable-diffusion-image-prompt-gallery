#!/bin/sh

group=$1
src="groups/$group"
title=$(cat "$src/title.txt")

cat <<EOF
### [$title](groups/$group)

<a href="groups/$group"><img loading="lazy" width="30%" src="groups/$group/1.png">&nbsp;<img loading="lazy" width="30%" src="groups/$group/2.png">&nbsp;<img loading="lazy" width="30%" src="groups/$group/3.png"></a>
EOF
